{
  "name": "giftable-frontend",
  "version": "1.0.0",
  "description": "The front-end for giftable",
  "main": "index.js",
  "scripts": {
    "build": "webpack --env prod --config webpack.config.babel.js && npm run css",
    "aws:clean": "aws s3 rm s3://www.giftableapp.co --recursive",
    "aws:sync-dist": "aws s3 sync dist s3://www.giftableapp.co",
    "aws:sync-assets": "aws s3 sync assets s3://www.giftableapp.co/assets",
    "deploy": "yarn build && yarn aws:clean && yarn aws:sync-dist && yarn aws:sync-assets",
    "dev": "yarn && yarn css && webpack-dev-server --progress --colors --env dev --watch --config ./webpack.config.babel.js & nodemon -e scss -x \"npm run css\"",
    "sass": "node-sass --include scss src/styles/styles.scss -o dist",
    "postcss": "postcss dist/styles.css -u autoprefixer -b \"last 2 versions\" -u cssnano -r",
    "css": "yarn sass | yarn postcss",
    "test": "jest"
  },
  "author": "Firman Cahyandi",
  "license": "ISC",
  "browserslist": [
    "last 2 versions"
  ],
  "dependencies": {
    "@storybook/addon-actions": "^3.3.15",
    "@storybook/addons": "^3.3.15",
    "@storybook/react": "^3.3.15",
    "clean-webpack-plugin": "^0.1.18",
    "history": "^4.7.2",
    "immutable": "^4.0.0-rc.10",
    "install": "^0.12.1",
    "lodash": "^4.17.5",
    "mori": "^0.3.2",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.11",
    "npm": "^6.4.1",
    "postcss-cli": "^6.0.0",
    "ramda": "^0.25.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "recompose": "^0.30.0",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-observable": "^1.0.0",
    "redux-persist": "^5.9.1",
    "reselect": "^3.0.1",
    "rxjs": "^6.3.2",
    "rxjs-compat": "^6.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "autoprefixer": "^9.1.5",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "cssnano": "^4.1.0",
    "enzyme": "^3.3.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.80.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jest-enzyme": "^6.0.0",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "^4.3.7",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^4.19.0",
