{
  "name": "giftable-frontend",
  "version": "1.0.0",
  "description": "The front-end for giftable",
  "main": "index.js",
  "scripts": {
    "build": "webpack --env prod --config webpack.config.babel.js && npm run css",
    "aws:clean": "aws s3 rm s3://www.giftableapp.co --recursive",
    "aws:sync-dist": "aws s3 sync dist s3://www.giftableapp.co",
    "aws:sync-assets": "aws s3 sync assets s3://www.giftableapp.co/assets",
    "deploy": "yarn aws:clean && yarn aws:sync-dist && yarn aws:sync-assets",
    "dev": "yarn && yarn css && webpack-dev-server --progress --colors --env dev --watch --config ./webpack.config.babel.js & nodemon -e scss -x \"npm run css\"",
    "sass": "node-sass --include scss src/styles/styles.scss -o dist",
    "autoprefixer": "postcss --use autoprefixer -b \"last 2 versions\" < dist/styles.css",
    "cssnano": "postcss --use cssnano > dist/styles.css",
    "css": "yarn sass | yarn autoprefixer | yarn cssnano",
    "test": "jest"
  },
  "author": "Firman Cahyandi",
  "license": "ISC",
  "dependencies": {
    "@storybook/addon-actions": "^3.3.15",
    "@storybook/addons": "^3.3.15",
    "@storybook/react": "^3.3.15",
    "clean-webpack-plugin": "^0.1.18",
    "history": "^4.7.2",
    "install": "^0.11.0",
    "lodash": "^4.17.5",
    "monet": "^0.8.10",
    "mori": "^0.3.2",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.11",
    "npm": "^5.8.0",
    "postcss-cli": "^5.0.0",
    "ramda": "^0.25.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-observable": "^0.18.0",
    "redux-persist": "^5.9.1",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "rxjs": "^5.5.8"
  },
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-universal-import": "^1.4.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "cssnano": "^3.10.0",
    "enzyme": "^3.3.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.67.1",
    "html-webpack-plugin": "^2.30.1",
